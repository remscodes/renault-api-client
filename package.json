{
  "name": "@remscodes/renault-api-client",
  "version": "1.0.1",
  "description": "Http client to use Renault API",
  "license": "MIT",
  "author": {
    "name": "RÃ©my Abitbol",
    "url": "https://github.com/remscodes"
  },
  "scripts": {
    "build": "rollup -c",
    "types": "tsc --noEmit",
    "types:spec": "npm run types -- -p ./tsconfig.spec.json",
    "types:all": "npm run types & npm run types:spec",
    "rm-dist": "node ./scripts/rm-dist.cjs",
    "prebuild": "npm run rm-dist",
    "pretest": "npm run types:all"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "node": "./dist/cjs/index.cjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/remscodes/renault-api-client"
  },
  "engines": {
    "node": ">=18.13.0"
  },
  "peerDependencies": {
    "@remscodes/renault-api": "^1.2.2",
    "dayjs": "^1.11.10",
    "drino": "^1.7.0",
    "thror": "^1.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.9.5",
    "esbuild": "^0.19.7",
    "rollup": "^4.5.1",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-esbuild": "^6.1.0",
    "typescript": "^5.3.2"
  },
  "keywords": [
    "renault",
    "http"
  ],
  "packageManager": "pnpm@8.10.5"
}
